 => {\n    let map = tt.map({\n      /* \n      This key will API key only works on this Stackblitz. To use this code in your own project,\n      sign up for an API key on the TomTom Developer Portal.\n      */\n      key: `${TOMTOMAPIKEY}`,\n      container: mapElement.current,\n      center: [mapLongitude, mapLatitude],\n      zoom: mapZoom\n    });\n    setMap(map);\n    return () => map.remove();\n  }, []);\n\n  return (\n    <div >\n      <nav  style={{ backgroundColor: \"#4287f5\" }}>\n        <p>TomTom Maps + React = ðŸ˜ƒ</p>\n      </nav>\n      <div >\n        <section>\n          <h4>Map Controls</h4>\n          <section>\n            <label htmlFor=\"longitude\">Longitude</label>\n            <input\n              type=\"text\"\n              name=\"longitude\"\n              value={mapLongitude}\n              onChange={(e) => setMapLongitude(e.target.value)}\n            />\n          </section>\n          <section>\n            <label htmlFor=\"latitude\">Latitude</label>\n            <input\n              type=\"text\"\n              name=\"latitude\"\n              value={mapLatitude}\n              onChange={(e) => setMapLatitude(e.target.value)}\n            />\n          </section>\n          <section>\n            <p>Zoom</p>\n\n            <button  onClick={decreaseZoom}>\n              -\n            </button>\n            <div className=\"mapZoomDisplay\">{mapZoom}</div>\n            <button onClick={increaseZoom}>\n              +\n            </button>\n          </section>\n          <section>\n            <p className=\"updateButton\"></p>\n            <button  onClick={updateMap}>\n              Update Map\n            </button>\n          </section>\n        </section>\n        <section >\n          <div ref={mapElement} className=\"mapDiv\" />\n        </section>\n      </div>\n    </div>\n  );\n}\n \nexport default App;\n","/home/monica/Documentos/Curso/reto/sustainable-mobility/src/reportWebVitals.js",[],[],{"ruleId":"19","severity":1